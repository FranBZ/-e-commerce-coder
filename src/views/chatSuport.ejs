<!DOCTYPE html>
<html lang='en'>
<%- include('partials/head') %>
<body>
    <%- include('partials/nav') %>
    <div class='container mt-5'>
        <div class='row'>

            <div class='col' id="messages-center">
                <div class="text-bg-dark p-3" style="margin-bottom: 5px;">Centro de mensajes</div>
                <div class="input-group mb-3">
                    <span class="input-group-text">Email</span>
                    <input type="text" class="form-control" placeholder="Nombre de usuario" aria-label="Username" aria-describedby="basic-addon1" id="email">
                </div>
                <div class="input-group mb-3" id="chat"></div>
                <div class="input-group" style="margin-bottom: 5px;">
                    <span class="input-group-text">Su mensaje:</span>
                    <textarea class="form-control" aria-label="With textarea" id="message"></textarea>
                </div>
                <button type='button' class='btn btn-primary' id="send">Enviar</button>
            </div>
            
        </div>
    </div>
    <script src="js/main.js"></script>
</body>
</html>